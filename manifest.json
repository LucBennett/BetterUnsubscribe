      {
        "light": "icons/email_48x48_inverted.png",
        "dark": "icons/email_48x48.png",
        "size": 48
      },
      {
        "light": "icons/email_inverted.png",
        "dark": "icons/email.png",
        "size": 64
      }
    ]
  },
  "manifest_version": 2
}
