{
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "2.6.0",
  "default_locale": "en",
  "author": "Luc Bennett",
  "browser_specific_settings": {
    "gecko": {
      "id": "{4753278b-acea-4b2b-a111-1fc9450d239d}",
      "strict_min_version": "115.0"
    }
  },
  "permissions": [
    "accountsRead",
